<script>(function() {
    let quickApiKey = document.getElementById('api-key-show')
    if (quickApiKey) {
        quickApiKey.addEventListener('click', function(e) {
            quickApiKey.style.display = 'none';
            document.getElementById('api-key-content').style.display = 'block';
        })
    }
})()
    </script>
<script>(function() {
    let $menuBtn = document.getElementById('menu-btn')
    document.body.addEventListener('click', function(e) {
        // Only check when the menu is open
        if (!$menuBtn.checked) {
            return
        }

        let elm = e.target
        let showMenu = false
        while (elm) {
            // Ignore duplicated event
            if (elm.classList.contains('menu-icon')) {
                return
            }

            // Toggle menu when clicking on the menu icon
            if (elm.id == 'menu-btn') {
                showMenu = true
                break
            }
            elm = elm.parentElement
        }

        $menuBtn.checked = showMenu
    })
})()
    </script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.23.0/prism.min.js" defer="defer"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/prism/1.23.0/plugins/autoloader/prism-autoloader.min.js" defer="defer"></script>
<script>//- Hide/ show API document example blocks
function ready(fn) {
    if (document.readyState != 'loading'){
        fn();
    } else {
        document.addEventListener('DOMContentLoaded', fn);
    }
}
ready(function () {
    function hideExample(example, displayId) {
        //- Hide/ show code blocks for each examples
        let codes = example.querySelectorAll('div.code');
        for (let i = 0; i < codes.length; i++) {
            if (codes[i].id !== displayId) {
                codes[i].style.display = 'none';
            } else {
                codes[i].style.display = 'block';
            }
        }
    }

    //- Get all example blocks
    let examples = document.querySelectorAll('.examples');
    for (let i = 0; i < examples.length; i++) {
        let example = examples[i];
        let language = example.querySelector('.languages');
        if (language) {
            let selectEl = language.querySelector('select');
            let displayId = selectEl.value + '-example';

            //- Hide not selected example blocks
            hideExample(example, displayId);

            //- When user change language
            example.addEventListener('change', function(event) {
                let displayId = event.target.value + '-example';
                hideExample(example, displayId);
            });
        }

        //- Get all copy buttons
        let copyButtons = example.querySelectorAll('.copy-button');
        for (let i = 0; i < copyButtons.length; i++) {
            let copyButton = copyButtons[i];

            //- Add tooltip
            copyButton.addEventListener('mouseenter', function() {
                let copyTooltip = document.createElement('div');
                copyTooltip.setAttribute('class', 'copy-tooltip bg-dark');
                copyTooltip.innerHTML = '<div class="arrow"></div><div class="content">Click to copy</div>';
                copyButton.appendChild(copyTooltip);
            });

            //- Remove tooltip
            copyButton.addEventListener('mouseleave', function() {
                copyButton.querySelector('.copy-tooltip').remove();
            });

            //- Copy example code
            copyButton.addEventListener('click', function() {
                let exampleCode = copyButton.parentElement.nextElementSibling;

                //- Create temporary textArea as document.execCommand only work on textarea, input
                let textArea = document.createElement('textarea');
                let exampleText = exampleCode.innerText.trim();
                //- We added $ at the beginning for Bash language, so try to remove it first
                exampleText = exampleText.replace(/^\$ /, "");

                //- Set text and focus
                textArea.value = exampleText;
                document.body.appendChild(textArea);
                textArea.select();

                //- Copy to clipboard
                try {
                    document.execCommand('copy');
                    //- Change tooltip content
                    copyButton.querySelector('.content').innerHTML = '<strong>Copied</strong>';
                } catch (err) {
                    console.log('Oops, unable to copy', err);
                }

                //- Remove temporary textArea
                document.body.removeChild(textArea);
            });
        }
    }

    //- Hide/ show API request blocks
    //- Get all .method bars that contain description target
    let methods = document.querySelectorAll('.method[data-target]');
    for (let i = 0; i < methods.length; i++) {
        //- Get target Id and remove # character at first
        let targetId = methods[i].getAttribute('data-target').replace(/^#/, '');
        let description = document.getElementById(targetId);

        if (description) {
            //- Hide all description sections on first load
            description.style.display = 'none';

            //- Handle click event
            methods[i].addEventListener('click', function(event) {
                //- Hide/ show the description
                if (description.style.display === 'block') {
                    description.style.display = 'none';
                } else {
                    description.style.display = 'block';
                }
            });
        }
    }
});
</script>
</body>
